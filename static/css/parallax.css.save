.space-parallax {
	width: 100%;
	height: 100%;
	display: float;
	position: absolute;
	background-image: url("/static/img/parallax1.png"), url("/static/img/parallax2.png"), url("/static/img/parallax3.png");
	background-repeat: repeat, repeat, repeat;
	background-blend-mode: lighten, lighten, lighten;
	animation: space-parallax linear 100s infinite forwards;
	z-index: -100;
}

@keyframes space-parallax {
	from {
		background-position: 0px 0px, 0px 0px, 0px 0px;
	}

	to {
		background-position: -2880px 0px, -4800px 0px, -6720px 0px;
	}
}
